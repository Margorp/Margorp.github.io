<?xml version="1.0" encoding="utf-8" ?> 
<program version='1.0'
>
    <reference/>
    <main>
        <constant:set name='list_constant' type='list' value='[1,2,3,"a","b","c",4,5,6]'/>
        
        <for:i name='i' type="int" from="0i" to="${list_constant()::Count()}">
          <print:line type="int" value="${i}"/>
        </for:i>

        <debug:print type="int" value="${list_constant[0]}==1"/>
        <debug:print type="int" value="${list_constant[1]}==2"/>
        <debug:print type="int" value="${list_constant[2]}==3"/>
        <debug:print type="int" value='"${list_constant[3]}"=="a"'/>
        <debug:print type="int" value='"${list_constant[4]}"=="b"'/>
        <debug:print type="int" value='"${list_constant[5]}"=="c"'/>
        <debug:print type="int" value='${list_constant[6]}==4'/>
        <debug:print type="int" value='${list_constant[7]}==5'/>
        <debug:print type="int" value='${list_constant[8]}==6'/>

        <variable:set name="j" type="int" value="0i"/>
        <variable:set name='items' type='object' value='[]'/>
        <for:i name='i' type="int" from="0i" to="${list_constant()::Count()}" step = "2i">
          <print:line type="object" value="${list_constant[${i}]}"/>
          <add:list name="items" type="list" value="${list_constant[${i}]}"/>
        </for:i>

        <for:each name='item' type="object" value="${items}">
          <dump:_ type="object" value="${item}"/>
        </for:each>

        <debug:print type="int" value="${items[0]}==1"/>
        <debug:print type="int" value="${items[1]}==3"/>
        <debug:print type="int" value='"${items[2]}"=="b"'/>
        <debug:print type="int" value='${items[3]}==4'/>
        <debug:print type="int" value='${items[4]}==6'/>
    </main>
</program>
